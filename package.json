{
  "name": "indebox",
  "pretty_name": "Indebox",
  "version": "0.0.0",
  "description": "A middlehand between InDesign and Dropbox to make them work together",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack --watch",
    "test": "osascript .tmp/app.js",
    "build": "webpack && osacompile -o dist/Indebox.app -l JavaScript .tmp/app.min.js && npm run build:icon && npm run build:copy && npm run build:template",
    "build:icon": "iconutil -c icns -o icons/src/applet.icns icons/applet.iconset && iconutil -c icns -o icons/src/document_icon.icns icons/document_icon.iconset",
    "build:template": "mustache package.json templates/info.mustache > dist/Indebox.app/Contents/Info.plist",
    "build:copy": "cp -a icons/src/. dist/Indebox.app/Contents/Resources"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adambrgmn/indebox.git"
  },
  "keywords": [
    "JXA",
    "Webpack",
    "Babel",
    "ES2015",
    "Eslint"
  ],
  "author": {
    "name": "Adam Bergman",
    "email": "adam@fransvilhelm.com",
    "url": "http://fransvilhelm.com/"
  },
  "contributors": [
    "Adam Bergman <adam@fransvilhelm.com> (http://fransvilhelm.com/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adambrgmn/indebox/issues",
    "email": "adam@fransvilhelm.com"
  },
  "engines": {
    "node": ">=5.10.1",
    "npm": ">=3.8.5"
  },
  "homepage": "https://github.com/adambrgmn/indebox#readme",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "clean-webpack-plugin": "^0.1.8",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-loader": "^1.3.0",
    "json-loader": "^0.5.4",
    "mustache": "^2.2.1",
    "uglify-js": "^2.6.2",
    "webpack": "^1.13.0",
    "webpack-merge": "^0.12.0"
  },
  "dependencies": {
    "left-pad": "^1.0.2",
    "lodash": "^4.11.1",
    "path": "^0.12.7"
  }
}
